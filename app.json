{
  "expo": {
    "name": "Exire",
    "slug": "rntut",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#3597e9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "ai.exire.exireMobile",
      "buildNumber": "2",
      "infoPlist": {
        "NSContactsUsageDescription": "You can directly invite friends through you contact list."
      }
    },
    "android": {
      "package": "ai.exire.exireMobile",
      "googleServicesFile": "./googleDocs/google-services.json"
    }
  }
}
